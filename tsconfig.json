{
  "compilerOptions": {
    "sourceMap": true,    
    "alwaysStrict": true,
    "strict": true,
    "noImplicitAny": true,
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    // "no-use-before-define": ["error", {
    //     "functions": true,
    //     "classes": true,
    //     "variables": true,
    //     "allowNamedExports": false
    // }],
  }
}